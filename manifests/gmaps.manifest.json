{
  "manifest_version": 2,
  "key": "gmaps",
  "name": "Google Maps",
  "description": "Collects Google Maps place reviews",
  "version": "1.0.0",
  "file": "connectors/gmaps.ts",
  "authSchema": {
    "methods": [
      {
        "type": "env_keys",
        "required": true,
        "scope": "source",
        "fields": [
          {
            "key": "GOOGLE_MAPS_API_KEY",
            "label": "Google Maps API Key",
            "example": "AIzaSyD8rL2mQ5pV9xC4nB7kT1wF6yH3uJ0sA2",
            "secret": true
          }
        ]
      }
    ]
  },
  "optionsSchema": {
    "description": "Google Maps connector options - requires either place_id or business_name",
    "$id": "GoogleMapsOptions",
    "type": "object",
    "properties": {
      "place_id": {
        "description": "Google Place ID for the business",
        "example": "ChIJN1t_tDeuEmsRUsoyG83frY4",
        "minLength": 1,
        "type": "string"
      },
      "business_name": {
        "description": "Business name for search-based fallback if place_id not provided",
        "example": "Blue Bottle Coffee",
        "minLength": 1,
        "type": "string"
      }
    }
  }
}
